pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                // Checkout your code repository
                git 'https://github.com/srinathsr71/demo_jenkins_selenium_pipeline.git'
            }
        }
              
        stage('Test') {
            steps {
                // Run your Selenium test case
                sh 'mvn clean test' // Or any other command to run your test
                 }
        }
        
        stage('Publish Results') {
            steps {
                // Publish TestNG results
                step([$class: 'TestNGPublisher', 
                      reportFilenamePattern: 'target/surefire-reports/testng-results.xml'])
            }
        }
    }
    
  
